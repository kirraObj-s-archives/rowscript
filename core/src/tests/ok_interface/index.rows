function numAdd(a: number, b: number): number;
function numMul(a: number, b: number): number;

type Foo = {n: number};

interface NatLike {
    add(a: NatLike, b: NatLike): NatLike;
    mul(a: NatLike, b: NatLike): NatLike;
}

implements NatLike for Foo {
    add(a: Foo, b: Foo): Foo {
        return {n: numAdd(a.n, b.n)}
    }

    mul(a: Foo, b: Foo): Foo {
        return {n: numMul(a.n, b.n)}
    }
}

function f0(): number {
    return add({n: 42}, {n: 69}).n
}

function f1(): number {
    return f0()
}

interface Default { default(): Default; }
implements Default for Foo { default(): Foo { return {n: 0} } }
function g0(): Foo { return default() }
function g1(): Foo { return g0() }

function h1<T>(a: T): T
//where
//    T: NatLike
{
    return add(a, a)
}
