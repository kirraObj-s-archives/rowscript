interface Functor {
    map<A, B>(f: (a: A) -> B, x: Functor<A>): Functor<B>;
}

type Foo<T> = {n: T};

implements Functor for Foo {
    map<A, B>(f: (a: A) -> B, x: Foo<A>): Foo<B> {
        return {n: f(x.n)}
    }
}

function numToStr(n: number): string;

function f(): string {
    return map(numToStr, {n: 42})
}
